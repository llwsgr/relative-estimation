{
  "extends": [
    "google",
    "prettier",
    "prettier/@typescript-eslint",
    "prettier/react",
    "plugin:react/recommended"
  ],
  "rules": {
    // Additional, per-project rules...
    "require-jsdoc" : 0,
    "valid-jsdoc" : 0,
    "react/prop-types": 0,
    "react/jsx-uses-vars": 1,
    "react/react-in-jsx-scope": 1
  },
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "env": {
    "es6": true
  }
}
